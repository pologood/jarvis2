<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<strategies>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.CpuAcceptanceStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.JobNumAcceptionStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.LoadAcceptanceStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.MemoryAcceptanceStrategy</strategy>
	</strategies>
	
	<jobs>
	    <job type="hive_shell" class="com.mogujie.jarvis.tasks.shell.HiveSqlShellTask">
	    		<strategies>
				<strategy>com.mogujie.jarvis.worker.strategy.impl.YarnMemoryAcceptanceStrategy</strategy>
			</strategies>
	    </job>
	    <job type="hive_script" class="com.mogujie.jarvis.tasks.shell.HiveScriptShellTask">
	    		<strategies>
				<strategy>com.mogujie.jarvis.worker.strategy.impl.YarnMemoryAcceptanceStrategy</strategy>
			</strategies>
	    </job>
	    
	    <job type="shell" class="com.mogujie.jarvis.tasks.shell.ShellJob" />
	    <job type="presto" class="com.mogujie.jarvis.tasks.jdbc.PrestoJdbcJob" />
	    <job type="dummy" class="com.mogujie.jarvis.tasks.DummyJob" />
	</jobs>
</configuration>