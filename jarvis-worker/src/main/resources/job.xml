<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<strategies>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.CpuAcceptionStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.JobNumAcceptionStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.LoadAcceptionStrategy</strategy>
		<strategy>com.mogujie.jarvis.worker.strategy.impl.MemoryAcceptionStrategy</strategy>
	</strategies>
	
	<jobs>
	    <job type="hive" class="com.mogujie.jarvis.jobs.impl.HiveJob">
	    	<strategies>
				<strategy>com.mogujie.jarvis.worker.strategy.impl.YarnMemoryAcceptionStrategy</strategy>
			</strategies>
	    </job>
	    
	    <job type="shell" class="com.mogujie.jarvis.jobs.impl.ShellJob" />
	    <job type="presto" class="com.mogujie.jarvis.jobs.impl.PrestoJdbcJob" />
	</jobs>
</configuration>